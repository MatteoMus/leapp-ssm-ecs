{
  "name": "leapp-ssm-ecs-exec",
  "author": { 
    "name": "matteo.mus",
    "email": "matteo.mus.mm@gmail.com" 
  },
  "version": "1.0.2",
  "description": "Execute a command in your AWS ECS container through SSM",
  "keywords": [
    "leapp-plugin",
    "AWS ECS",
    "SSM"
  ],
  "leappPlugin": {
    "supportedOS": [
      "mac", "linux"
    ],
    "supportedSessions": [
      "awsIamRoleFederated",
      "awsIamRoleChained",
      "awsSsoRole",
      "awsIamUser",
      "aws"
    ]
  },
  "scripts": {
    "build": "webpack --config webpack.config.js"
  },
  "files": [
    "plugin.js", 
    "icon.png",
    "ecs.sh"
  ],
  "license": "MPL",
  "dependencies": {
    "@noovolari/leapp-core": "0.1.145"
  },
  "devDependencies": {
    "copy-webpack-plugin": "^11.0.0",
    "path-browserify": "^1.0.1",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^5.0.1"
  }
}
